{
  "name": "starter-project-eleventy-github-pages",
  "version": "1.0.0",
  "description": "Starter Project para crear SSGs con 11ty y desplegarlos en GitHub Pages",
  "main": ".eleventy.js",
  "scripts": {
    "install": "npm install @11ty/eleventy --save-dev",
    "start": "npx @11ty/eleventy --serve --port=7777",
    "clean": "rm -rf docs",
    "deploy": "npx @11ty/eleventy --pathprefix=/",
    "deploy-subdomain": "npx @11ty/eleventy --pathprefix=/subdirectory/ && npx pagefind --source=docs"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "github pages"
  ],
  "author": "Karim",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "pagefind": "^1.2.0"
  },
  "dependencies": {
    "liquidjs": "^10.19.0"
  
  }
}
